    <validationRules>
        <fullName>Prevent_Edit_Audit_findings_cancel_audit</fullName>
        <active>true</active>
        <errorConditionFormula>Audit__r.CMPL123_WF_Status__c =="Canceled - Audit"</errorConditionFormula>
        <errorMessage>Record Locked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_edit_create_audit_is_approved</fullName>
        <active>false</active>
        <errorConditionFormula>Audit__r.Is_Audit_Approved__c</errorConditionFormula>
        <errorMessage>Audit findings cannot be added or edited after audit has been approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Response_should_not_be_blank</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(CMPL123_WF_Action__c)&amp;&amp; CMPL123_WF_Action__c =="Submit Response" &amp;&amp; ISBLANK( Response__c )</errorConditionFormula>
        <errorMessage>Please fill the Response field before performing the Submit response action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>do_not_add_new_findings_on_closed_audit</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Audit__r.CMPL123_WF_Action__c == "Close" , ISNEW() )</errorConditionFormula>
        <errorMessage>Cannot Add a New Audit Findings for the Closed Audit</errorMessage>
    </validationRules>
